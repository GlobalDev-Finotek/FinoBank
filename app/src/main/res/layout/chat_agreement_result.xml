<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:orientation="vertical"
              android:layout_width="match_parent"
              android:layout_height="wrap_content"
              android:layout_marginStart="15dp"
              android:layout_marginEnd="15dp"
              android:layout_marginBottom="15dp"
              android:background="@drawable/border_round_gray">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="1dp"
        android:layout_marginStart="1dp"
        android:layout_marginEnd="1dp"
        android:background="@android:color/white"
        android:padding="15dp">
        <TextView
            android:layout_alignParentTop="true"
            android:layout_alignParentStart="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/dark_100"
            android:textSize="14sp"
            android:text="대출 서비스 이용 약관.pdf" />
        <Button
            android:layout_alignParentTop="true"
            android:layout_alignParentStart="true"
            android:layout_width="wrap_content"
            android:layout_height="20dp"
            android:layout_marginTop="25dp"
            android:paddingStart="12dp"
            android:paddingEnd="12dp"
            android:textSize="12sp"
            android:textColor="@color/dark_100"
            android:text="@string/string_preview"
            android:background="@drawable/btn_small_round_default" />
        <Button
            android:layout_alignParentTop="true"
            android:layout_alignParentStart="true"
            android:layout_width="50dp"
            android:layout_height="20dp"
            android:layout_marginTop="25dp"
            android:layout_marginStart="94dp"
            android:paddingStart="12dp"
            android:paddingEnd="12dp"
            android:textSize="12sp"
            android:textColor="@color/dark_100"
            android:text="@string/string_save"
            android:background="@drawable/btn_small_round_default" />
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@android:color/white"
        android:padding="15dp"
        android:layout_marginTop="1dp"
        android:layout_marginStart="1dp"
        android:layout_marginEnd="1dp">
        <TextView
            android:layout_alignParentTop="true"
            android:layout_alignParentStart="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/dark_100"
            android:textSize="14sp"
            android:text="개인(신용정보 조회 및 이용 제공 동의.pdf" />
        <Button
            android:layout_alignParentTop="true"
            android:layout_alignParentStart="true"
            android:layout_width="wrap_content"
            android:layout_height="20dp"
            android:layout_marginTop="25dp"
            android:paddingStart="12dp"
            android:paddingEnd="12dp"
            android:textSize="12sp"
            android:textColor="@color/dark_100"
            android:text="@string/string_preview"
            android:background="@drawable/btn_small_round_default" />
        <Button
            android:layout_alignParentTop="true"
            android:layout_alignParentStart="true"
            android:layout_width="50dp"
            android:layout_height="20dp"
            android:layout_marginTop="25dp"
            android:layout_marginStart="94dp"
            android:paddingStart="12dp"
            android:paddingEnd="12dp"
            android:textSize="12sp"
            android:textColor="@color/dark_100"
            android:text="@string/string_save"
            android:background="@drawable/btn_small_round_default" />
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@android:color/white"
        android:padding="15dp"
        android:layout_marginTop="1dp"
        android:layout_marginStart="1dp"
        android:layout_marginEnd="1dp">
        <TextView
            android:layout_alignParentTop="true"
            android:layout_alignParentStart="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/dark_100"
            android:textSize="14sp"
            android:text="대출거래 약정서.pdf" />
        <Button
            android:layout_alignParentTop="true"
            android:layout_alignParentStart="true"
            android:layout_width="wrap_content"
            android:layout_height="20dp"
            android:layout_marginTop="25dp"
            android:paddingStart="12dp"
            android:paddingEnd="12dp"
            android:textSize="12sp"
            android:textColor="@color/dark_100"
            android:text="@string/string_preview"
            android:background="@drawable/btn_small_round_default" />
        <Button
            android:layout_alignParentTop="true"
            android:layout_alignParentStart="true"
            android:layout_width="50dp"
            android:layout_height="20dp"
            android:layout_marginTop="25dp"
            android:layout_marginStart="94dp"
            android:paddingStart="12dp"
            android:paddingEnd="12dp"
            android:textSize="12sp"
            android:textColor="@color/dark_100"
            android:text="@string/string_save"
            android:background="@drawable/btn_small_round_default" />
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@android:color/white"
        android:padding="15dp"
        android:layout_margin="1dp">
        <TextView
            android:layout_alignParentTop="true"
            android:layout_alignParentStart="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/dark_100"
            android:textSize="14sp"
            android:text="계약 안내사항 및 유의사항.pdf" />
        <Button
            android:layout_alignParentTop="true"
            android:layout_alignParentStart="true"
            android:layout_width="wrap_content"
            android:layout_height="20dp"
            android:layout_marginTop="25dp"
            android:paddingStart="12dp"
            android:paddingEnd="12dp"
            android:textSize="12sp"
            android:textColor="@color/dark_100"
            android:text="@string/string_preview"
            android:background="@drawable/btn_small_round_default" />
        <Button
            android:layout_alignParentTop="true"
            android:layout_alignParentStart="true"
            android:layout_width="50dp"
            android:layout_height="20dp"
            android:layout_marginTop="25dp"
            android:layout_marginStart="94dp"
            android:paddingStart="12dp"
            android:paddingEnd="12dp"
            android:textSize="12sp"
            android:textColor="@color/dark_100"
            android:text="@string/string_save"
            android:background="@drawable/btn_small_round_default" />
    </RelativeLayout>
</LinearLayout>